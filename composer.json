{
    "name": "luismabenitez/beacon",
    "description": "Error monitoring and reporting package for Laravel - Rocketfy internal Flare alternative",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Luis Miguel Benitez",
            "email": "luisma@rocketfy.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Luismabenitez\\Beacon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luismabenitez\\Beacon\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luismabenitez\\Beacon\\BeaconServiceProvider"
            ],
            "aliases": {
                "Beacon": "Luismabenitez\\Beacon\\Facades\\Beacon"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
